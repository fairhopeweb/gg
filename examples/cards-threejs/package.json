{
  "name": "cards",
  "version": "1.0.0",
  "description": "cards example",
  "main": "dist/cards.js",
  "author": "Corban Riley",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "prettier": "prettier --list-different --write src/**/*.ts"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.22",
    "@types/stats": "^0.16.30",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "vite": "^5.2.6"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "popmotion": "^8.5.3",
    "sillyname": "^0.1.0",
    "stats.js": "^0.17.0",
    "three": "^0.114.0",
    "unistore": "^3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": "/__tests__/.*\\.test\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  }
}
